<div class="fields">
<br/>
<% if selected_type.nil?%>
	<%= select_tag "#{object_type}[subjectType][]",  options_for_select(subjectTypeArray) , :prompt=>"Select Subject Type" , :onChange=>"javascript: getSimpleEditSubjects(this,'#{object_type}','#newClassName');"%>
	<div id="newClassName"></div>
<% else %>
	<%= hidden_field_tag "#{object_type}[subjectType][]", "#{selected_type}" %>
	<script type="text/javascript">
	   getSimpleSubjects("<%= selected_type %>","<%= object_type %>","#newClassName<%= index %>","0","<%= selected_value %>");
	</script>
	<div id="newClassName<%= index %>"></div>
	</div>	
<% end %>
