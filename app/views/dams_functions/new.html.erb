<h1>Create a new Dams Function</h1>
<%= form_for @dams_function do |f| %>
	<%= render 'error_messages' %>
  <%= label_tag 'dams_function[name]', "Name" %>
  <%= text_field_tag 'dams_function[name]' %>
  <%= label_tag 'dams_function[externalAuthority]', "ExternalAuthority" %>
  <%= url_field_tag 'dams_function[externalAuthority]' %> 
  <%= label_tag 'dams_function[elementValue]', "FunctionElement" %>
  <%= text_field_tag 'dams_function[elementValue]' %> 
  <br>
  <%= select_tag 'dams_function[scheme]', options_for_select(@mads_schemes.collect{ |v| [v.name.first, v.pid] } ) , :prompt=>"Select MadsScheme" %>    
  <br>

  <%=f.submit "Submit", class: "btn btn-primary" %>
<%end%>
