<h1><%= @dams_function.name.first %></h1>

<div>
  <dl>
    <dt>FunctionElement:</dt>
    <dd><%= @dams_function.elementList[0].elementValue %></dd>
    <dt>Scheme:</dt>
    <dd><%= @dams_function.scheme.first.pid %></dd>
    <dt>ExternalAuthority:</dt>
    <dd><%= @dams_function.externalAuthority.first %></dd>
  </dl>
  <%= render :partial => 'shared/edit_fields/view_form_hidden_fields' , :locals => {:object => @dams_function, :parent_id => params[:parent_id], :parent_class => params[:parent_class]}%>

</div>
