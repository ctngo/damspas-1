<h1>Edit Dams Role</h1>
<%= form_for @dams_role do |f| %>
  <%= f.label :code %>
  <%= f.text_field :code %>
  <br>
  <%= f.label :value %>
  <%= f.text_field :value %>
  <br>
  <%= f.submit "Save", :class => :btn %>
  <%= link_to "Cancel", dams_role_path(@dams_role), :class => :btn %>
  <%= link_to "View All", dams_roles_path(), :class => :btn %>
<% end %>

