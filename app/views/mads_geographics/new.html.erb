<h1>Create a new MADS Geographic</h1>
<%= form_for @mads_geographic do |f| %>
	<%= render 'error_messages' %>
  <%= label_tag 'mads_geographic[name]', "Name" %>
  <%= text_field_tag 'mads_geographic[name]' %>
	<%= label_tag 'mads_geographic[authority]', "Code" %>
  <%= text_field_tag 'mads_geographic[authority]' %>
  <%= label_tag 'mads_geographic[valueURI]', "URI" %>
  <%= url_field_tag 'mads_geographic[valueURI]' %>
  <br>

  <%=f.submit "Submit", class: "btn btn-primary" %>
<%end%>
