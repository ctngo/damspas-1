<h1>Create a new MADS GenreForm</h1>
<%= form_for @mads_genre_form do |f| %>
	<%= render 'error_messages' %>
  <%= label_tag 'mads_genre_form[name]', "Name" %>
  <%= text_field_tag 'mads_genre_form[name]' %>
	<%= label_tag 'mads_genre_form[authority]', "Code" %>
  <%= text_field_tag 'mads_genre_form[authority]' %>
  <%= label_tag 'mads_genre_form[valueURI]', "URI" %>
  <%= url_field_tag 'mads_genre_form[valueURI]' %>
  <br>

  <%=f.submit "Submit", class: "btn btn-primary" %>
<%end%>
