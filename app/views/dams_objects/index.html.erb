<table class="table table-striped">
  <thead>
    <tr>
      <td>ID</td>
      <td>Title</td>
      <td>Related Title</td>
      <td>Related Title Type</td>
      <td>&nbsp;</td>
      <td>&nbsp;</td>
    </tr>
  </thead>

  <tbody>
    <% @dams_objects.each do |dams_object| %>

      <tr>
        <td class="zombie_nickname"><%= dams_object.id%></td>
        <td class="zombie_name"><%= link_to dams_object.title, dams_object_path(dams_object) %></td>
        <td class="zombie_name"><%= dams_object.relatedTitle%></td>
        <td class="zombie_name"><%= dams_object.relatedTitleType%></td>
        <td class="zombie_edit"><%= link_to "Edit", edit_dams_object_path(dams_object) %></td>
        <td>
          <%= link_to "Delete", dams_object_path(dams_object), :method => "delete" %>
        </td>
      </tr>
    <% end %>
  </tbody>

</table>

<%= link_to "Create New Dams Object", new_dams_object_path, class: 'btn' %>
