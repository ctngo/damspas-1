<h1>Edit a Dams Object</h1>
<%= form_for @dams_object do |f| %>
  <%= label_tag 'dams_object[title][]', "First title" %>
  <%= text_field_tag 'dams_object[title][]', @dams_object.title[0] %>
  <%= label_tag 'dams_object[date][]', "Date" %>
  <%= text_field_tag 'dams_object[date][]', @dams_object.date[0] %>
  <%= label_tag 'dams_object[subject][]', "Subject" %>
  <%=text_field_tag 'dams_object[subject][]', @dams_object.subject[0] %>
  <br>
  <%=f.submit "Submit", class: "btn btn-primary" %>
<%end%>
