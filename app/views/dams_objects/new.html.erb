<h1>Create a new Dams Object</h1>
<%= form_for @dams_object do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
 <%= f.label :relatedTitle %>
  <%= f.text_field :relatedTitle %>
  <br>
 <%= f.label :relatedTitleLang %>
  <%= f.text_field :relatedTitleLang %>
  <br>
 <%= f.label :relatedTitleType %>
  <%= f.text_field :relatedTitleType %>
  <br>
 <%= f.label :beginDate %>
  <%= f.text_field :beginDate %>
  <br>
 <%= f.label :endDate %>
  <%= f.text_field :endDate %>
  <br>
 <%= f.label :date %>
  <%= f.text_field :date %>
  <br>
  <%= f.label :languageId %>
  <%= f.select(:languageId, options_from_collection_for_select(@dams_langs, :id, :language) , :prompt => 'Select Language') %>
  <br>
  <%= f.submit "Save" %>

<% end %>
