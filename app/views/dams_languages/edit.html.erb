<h1>Edit DAMS Language</h1>
<%= form_for @dams_language do |f| %>
  <%= label_tag 'dams_language[code]', "Code" %><%= text_field_tag 'dams_language[code][]', @dams_language.code[0] %>
  <%= label_tag 'dams_language[value]', "Value" %><%= text_field_tag 'dams_language[value][]', @dams_language.value[0] %>
  <%= label_tag 'dams_language[valueURI]', "Value URI" %><%= @dams_language.valueURI[0] %>

  <br>
  <%=f.submit "Save", class: "btn btn-primary" %>
  <%= link_to "Cancel", dams_language_path( @dams_language ), class: "btn btn-primary" %>
<%end%>
