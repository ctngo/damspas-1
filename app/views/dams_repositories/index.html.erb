<h1>Digital Library Collections</h1>

<div id="landingCarousel" class="carousel slide span6">
  <!-- Carousel items -->
  <div class="carousel-inner">
    <div class="active item">
		<%= image_tag "http://lorempixel.com/460/500" %>
		<div class="carousel-caption">
			<h4>Title: Slide 1</h4>
			<p>Creator: Teh Interwebs</p>
		</div>
	</div>
    <div class="item">
		<%= image_tag "http://lorempixel.com/g/460/500" %>
		<div class="carousel-caption">
			<h4>Title: Slide 2</h4>
			<p>Creator: Teh Interwebs</p>
		</div>
	</div>
    <div class="item">
		<%= image_tag "http://lorempixel.com/g/460/500?Asdf" %>
		<div class="carousel-caption">
			<h4>Title: Slide 3</h4>
			<p>Creator: Teh Interwebs</p>
		</div>
	</div>
  </div>

  <!-- Carousel nav -->
  <a class="carousel-control left" href="#landingCarousel" data-slide="prev">&lsaquo;</a>
  <a class="carousel-control right" href="#landingCarousel" data-slide="next">&rsaquo;</a>
</div>

<div class="span6">
<div class="well">
  <h2>Search</h2>
  
  <div>         
   	<%= render_search_bar  %>
  </div>
</div>

<div>
	<h2>Browse</h2>
	<ul>
      <%= render_browse_facet_links %>
	</ul>
</div>

<div>
	Some very interesting text about repositories
	<ul><%= render_repository_list %></ul>
</div>
<div>
	<% if can? :create, DamsObject %>
	<%= link_to "Create", new_dams_object_path %>
	<% end %>
</div>
</div>