<h1>Edit Dams Vocabulary</h1>
<%= form_for @dams_vocab do |f| %>
  <%= f.label :vocabDesc %>
  <%= f.text_field :vocabDesc %>
  <br>
  <%= f.submit "Save", :class => :btn %>
  <%= link_to "Cancel", dams_vocab_path(@dams_vocab), :class => :btn %>
  <%= link_to "View All", dams_vocabs_path(), :class => :btn %>
<% end %>

